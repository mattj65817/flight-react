{
  "name": "@mattj65817/flight-react",
  "version": "1.0.0",
  "description": "Cinira Flight components for React",
  "exports": {
    ".": [
      {
        "browser": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "./dist/cjs/index.js"
    ],
    "./package.json": "./package.json"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattj65817/flight-react.git"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-cjs.json && tsc -p ./tsconfig-types.json",
    "clean": "rm -rf ./build ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest"
  },
  "keywords": [],
  "author": "Matt Jensen <mjensen65816@gmail.com>",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.27",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.3",
    "react": ">=18.2.0",
    "semantic-release": "^22.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "release": {
    "plugins": [
      "@semantic-release/github",
      [
        "@semantic-release/npm"
      ]
    ]
  },
  "types": "./dist/types/index.d.ts",
  "jest": {
    "preset": "ts-jest",
    "reporters": [
      [
        "jest-junit",
        {
          "outputDirectory": "./build/report",
          "outputName": "test-results.xml"
        }
      ]
    ]
  }
}
